# stack cleaned csv files in command line using csvkit
$ csvstack 2008clean.csv 2009clean.csv 2010clean.csv 2011clean.csv 2012clean.csv 2013clean.csv 2014clean.csv 2015clean.csv 2016clean.csv 2017clean.csv 2018clean.csv > stacked_2008_2018.csv

#select only the columns we want
$ stacked_2008_2018.csv | csvcut -c case_no,decision_date,case_status,country_of_origin,class_of_admission,us_economic_sector,employer_name,employer_city,employer_state,employer_zip,naics_code,naics_title,pw_soc_code,pw_job_title,pw_level,pw_amount,pw_unit_of_pay,wage_offer_from,wage_offer_to,job_city,job_state > cut_2008_2018.csv

